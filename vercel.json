{
    "version": 2,
    "builds": [
      { "src": "src/**/*", "use": "@vercel/static-build" },
      { "src": "api/json-server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/json-server.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  